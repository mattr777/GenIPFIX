<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.collections.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.richards777.genipfix.Controller">
   <columnConstraints>
      <ColumnConstraints halignment="CENTER" />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints />
   </rowConstraints>
   <children>
       <Label text="Name" GridPane.columnIndex="1" />
       <Label text="Size (bytes)" GridPane.columnIndex="2" />
       <CheckBox fx:id="checkBox1" mnemonicParsing="false" selected="true" GridPane.rowIndex="1" />
       <ComboBox fx:id="elementName1" GridPane.columnIndex="1" GridPane.rowIndex="1" promptText="element name">
           <items>
               <FXCollections fx:factory="observableArrayList">
                   <String fx:value="sourceIPv4Address" />
                   <String fx:value="destinationIPv4Address" />
                   <String fx:value="packetDeltaCount" />
                   <String fx:value="octetDeltaCount" />
               </FXCollections>
           </items>
       </ComboBox>
       <TextField fx:id="elementSize1" promptText="element size" text="4" GridPane.columnIndex="2" GridPane.rowIndex="1" />

       <CheckBox fx:id="checkBox2" mnemonicParsing="false" selected="true" GridPane.rowIndex="2" />
       <ComboBox fx:id="elementName2" GridPane.columnIndex="1" GridPane.rowIndex="2" promptText="element name">
           <items>
               <FXCollections fx:factory="observableArrayList">
                   <String fx:value="sourceIPv4Address" />
                   <String fx:value="destinationIPv4Address" />
                   <String fx:value="packetDeltaCount" />
                   <String fx:value="octetDeltaCount" />
               </FXCollections>
           </items>
       </ComboBox>
       <TextField fx:id="elementSize2" promptText="element size" text="4" GridPane.columnIndex="2" GridPane.rowIndex="2" />

       <CheckBox fx:id="checkBox3" mnemonicParsing="false" selected="true" GridPane.rowIndex="3" />
       <ComboBox fx:id="elementName3" GridPane.columnIndex="1" GridPane.rowIndex="3" promptText="element name">
           <items>
               <FXCollections fx:factory="observableArrayList">
                   <String fx:value="sourceIPv4Address" />
                   <String fx:value="destinationIPv4Address" />
                   <String fx:value="packetDeltaCount" />
                   <String fx:value="octetDeltaCount" />
               </FXCollections>
           </items>
       </ComboBox>
       <TextField fx:id="elementSize3" promptText="element size" text="4" GridPane.columnIndex="2" GridPane.rowIndex="3" />

       <CheckBox fx:id="checkBox4" mnemonicParsing="false" selected="true" GridPane.rowIndex="4" />
       <ComboBox fx:id="elementName4" GridPane.columnIndex="1" GridPane.rowIndex="4" promptText="element name">
           <items>
               <FXCollections fx:factory="observableArrayList">
                   <String fx:value="sourceIPv4Address" />
                   <String fx:value="destinationIPv4Address" />
                   <String fx:value="packetDeltaCount" />
                   <String fx:value="octetDeltaCount" />
               </FXCollections>
           </items>
       </ComboBox>
       <TextField fx:id="elementSize4" promptText="element size" text="4" GridPane.columnIndex="2" GridPane.rowIndex="4" />

       <TextField fx:id="filename" promptText="filename" text="ipfix.pcap" GridPane.columnIndex="1" GridPane.rowIndex="6" />
       <Button fx:id="genButton" mnemonicParsing="false" onAction="#generateFile" prefHeight="25.0" prefWidth="147.0" text="Generate" GridPane.columnIndex="2" GridPane.rowIndex="6" />
   </children>
   <padding>
      <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
   </padding>
</GridPane>
